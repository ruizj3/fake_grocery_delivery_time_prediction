{
  "_instructions": [
    "Map your columns to the expected ML pipeline columns below.",
    "Use 'table.column' format to specify columns from any table.",
    "The system will automatically figure out how to JOIN the tables.",
    "delivery_time_minutes is CALCULATED from (delivered_at - created_at), don't map it."
  ],
  "column_mapping": {
    "order_id": {
      "maps_to": "orders.order_id",
      "description": "Unique order identifier"
    },
    "customer_id": {
      "maps_to": "orders.customer_id",
      "description": "Customer identifier"
    },
    "store_id": {
      "maps_to": "stores.store_id",
      "description": "Restaurant/store identifier"
    },
    "created_at": {
      "maps_to": "orders.created_at",
      "description": "When order was placed (datetime)"
    },
    "delivered_at": {
      "maps_to": "orders.delivered_at",
      "description": "When order was delivered (datetime, nullable)"
    },
    "latitude": {
      "maps_to": "stores.latitude",
      "description": "Pickup location latitude"
    },
    "longitude": {
      "maps_to": "stores.longitude",
      "description": "Pickup location longitude"
    },
    "delivery_latitude": {
      "maps_to": "orders.delivery_latitude",
      "description": "Dropoff location latitude"
    },
    "delivery_longitude": {
      "maps_to": "orders.deliverY_longitude",
      "description": "Dropoff location longitude"
    },
    "total": {
      "maps_to": "orders.total",
      "description": "Order value in dollars.cents"
    },
    "quantity": {
      "maps_to": "order_items.quantity",
      "description": "Number of items"
    }
